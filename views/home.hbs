<ul >
  {{#events}}
    <li>
        <h2 style="color:white;font-family:calibri">{{ title }}</h2>
        <p style="color:white;font-family:calibri">{{ description }}</p>
        <p style="color:white;font-family:calibri">{{ location }}</p> 
    <script type="text/javascript">
    var clicks = 0;
    function onClick() {
        clicks += 1;
        document.getElementById("clicks").innerHTML = clicks;
    };
    </script>
    <a style="cursor: pointer;" onClick="onClick()">Like</a>
    <a id="clicks">0</a>
 
    </li>  
  {{/events}}
</ul>
